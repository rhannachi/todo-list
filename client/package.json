    "@zodios/core": "^10.9.6",
    "axios": "^1.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@rhannachi/eslint-config-prettier": "^1.2.3",
    "@types/node": "^20.7.2",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "dependency-cruiser": "^15.0.0",
    "typescript": "^5.2.2",
    "zod": "^3.22.4",
    "zod-validation-error": "^2.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
